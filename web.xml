<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>TransactionCtrler</servlet-name>
  	<display-name>TransactionCtrler</display-name>
  	<description></description>
  	<servlet-class>com.lakshmi.web.TransactionCtrler</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TransferCtrl</servlet-name>
  	<display-name>TransferCtrl</display-name>
  	<description></description>
  	<servlet-class>com.lakshmi.web.TransferCtrl</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>TransactionCtrler</servlet-name>
  	<url-pattern>/transaction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TransferCtrl</servlet-name>
  	<url-pattern>/TransferCtrl</url-pattern>
  </servlet-mapping>
</web-app>
